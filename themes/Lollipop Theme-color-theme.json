{
  "name": "Lollipop Theme",
  "type": "dark",
  "author": "Liliana Costa",
  "colors": {
    "contrastActiveBorder": "#00000000",
    "contrastBorder": "#00000000",
    "editor.background": "#1d2433",
    "foreground": "#a7a5c9",
    "icon.foreground": "#a7a5c9ab",
    "editor.foreground": "#eeffff",
    "activityBarBadge.background": "#263238",
    "sideBarTitle.foreground": "#bbbbbb",
    "sideBar.background": "#10141E"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#40567c",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Conhetes de Tags",
      "scope": [
        "punctuation.definition",
        "punctuation.definition.tag",
        "punctuation",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "storage.type.function.arrow"
      ],
      "settings": {
        "foreground": "#64748B"
      }
    },
    {
      "name": "Tag Color",
      "scope": ["entity.name.tag", "storage.type.function"],
      "settings": {
        "foreground": "#FF66B6"
      }
    },
    {
      "name": "String Elements",
      "scope": ["string"],
      "settings": {
        "foreground": "#7FC97D"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "keyword.operator",
        "entity.name.function",
        "support.variable.property",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#7DD3FC"
      }
    },
    {
      "scope": ["text", "variable"],
      "settings": {
        "foreground": "#b7c3df"
      }
    },
    {
      "scope": ["keyword.control", "storage.modifier", "meta.object-literal"],
      "settings": {
        "foreground": "#EFD122"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control.conditional",
        "constant.numeric",
        "constant.language.boolean",
        "storage.type",
        "constant.language.php"
      ],
      "settings": {
        "foreground": "#BD93F9"
      }
    },
    {
      "name": "Italic Style",
      "scope": [
        "constant.language.boolean",
        "comment",
        "punctuation.definition.comment",
        "meta.function.expression",
        "storage.type.function",
        "constant.language.php"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
}
