{
  "name": "Lollipop Theme",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "function": {
      "foreground": "#F3E077"
    },
    "variable": "#A8B5D5",
    "variable.declaration": "#A8B5D5",
    "method": "#7DD3FC",
    "parameter": "#A8B5D5",
    "interface": "#F472B6",
    "type": "#F472B6",
    "enum": "#F472B6",
    "enumMember": "#ffd580",
    "property": "#ffd580",
    "class.static": "#ffd580",
    "property.readonly": "#ffd580",
    "selfParameter": "#F472B6",
    "namespace": "#7DD3FC"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5B6678",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#A5F2A3"
      }
    },
    {
      "scope": [
        "text",
        "variable",
        "variable.other",
        "meta.object-literal",
        "source.sql"
      ],
      "settings": {
        "foreground": "#A8B5D5"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#F472B6"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.id",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#BD93F9"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#A8B5D5"
      }
    },
    {
      "scope": [
        "constant.other.color",
        "support.constant.font-name",
        "meta.property-value.css"
      ],
      "settings": {
        "foreground": "#F3E077"
      }
    },
    {
      "scope": ["entity.other.attribute-name", "support.function.misc"],
      "settings": {
        "foreground": "#7DD3FC"
      }
    },
    {
      "scope": [
        "storage.type.function",
        "keyword",
        "keyword.operator.new",
        "keyword.operator.expression.of",
        "constant.numeric",
        "support.function.aggregate",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#BD93F9"
      }
    },
    {
      "scope": [
        "storage.type",
        "constant.language",
        "storage.modifier",
        "support.variable.property",
        "keyword.control.flow",
        "markup.underline.link.markdown"
      ],
      "settings": {
        "foreground": "#7DD3FC"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "entity.name.type",
        "variable.other.object",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#F3E077"
      }
    },
    {
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#F472B6"
      }
    },
    {
      "scope": ["support.function.construct.output"],
      "settings": {
        "foreground": "#BD93F9"
      }
    },
    {
      "scope": ["source.python"],
      "settings": {
        "foreground": "#BD93F9"
      }
    },
    {
      "scope": ["constant.other"],
      "settings": {
        "foreground": "#A8B5D5"
      }
    },
    {
      "scope": ["entity.name.section.markdown"],
      "settings": {
        "foreground": "#A5F2A3"
      }
    },
    {
      "scope": [
        "punctuation.definition",
        "punctuation.decorator",
        "punctuation.accessor",
        "punctuation.separator",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#64748B"
      }
    },
    {
      "name": "Italic Style",
      "scope": [
        "variable.parameter",
        "storage.type",
        "storage.type.enum",
        "entity.other.attribute-name.pseudo-class",
        "entity.other.attribute-name.pseudo-element"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ],

